#include "point.h"
Point::Point (){
	city = 0;
	a = 0;
	b = 0; 
}


Point::Point (int city_in, int x_in, int y_in){
        city = city_in;
        a = x_in
	b = y_in;
}

bool Point::operator==(Point B){
	if(city == B.city_num() && a == B.x() && b = B.y.())
		return true;
	return false; 
}

void Point::print(){
	cout <<"City Number: " << city <<endl << "X: " << a <<endl << "Y: " << b <<endl; 

}

